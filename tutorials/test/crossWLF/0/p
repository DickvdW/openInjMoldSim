FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0.1e6;

boundaryField
{
    wallIn
    {
        type            zeroGradient;
    }

    wallOut
    {
        type            zeroGradient;
    }

    wallUp
    {
        type            cyclic;
    }

    wallDown
    {
        type            cyclic;
    }


    frontAndBack
    {
        type            empty;
    }
}

// ************************************************************************* //
